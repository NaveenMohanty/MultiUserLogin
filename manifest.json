{
  "manifest_version": 3,
  "name": "Multi User Login",
  "version": "1.0",
  "description": "Opens a new tab with cleared cookies for a fresh login session.",
  "permissions": ["activeTab", "tabs", "cookies", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
